@using VSporMVC.Models;
@model SaloonsTanimlama



<form action="GuncelleTanim" method="post">
    <div class="box box-info">
       
        <!-- /.box-header -->
        <!-- form start -->
        <div class="form-horizontal">
            <div class="box-body">
                <input type="hidden" id="Id" name="Id" asp-for="Id">
                <div class="form-group">
                    <label for="setType" class="control-label">Kategori Adı</label>

                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="BussinessId" name="BussinessId" placeholder="Kategori Adı" required asp-for="BussinessId">
                        <input type="text" class="form-control" id="SaloonTypeId" name="SaloonTypeId" placeholder="Kategori Adı" required asp-for="SaloonTypeId">
                        <input type="text" class="form-control" id="M2" name="M2" placeholder="Kategori Adı" required asp-for="M2">
                       
                </div>
                <div class="form-group">
                    <div class="col-sm-12">

                        <button type="submit" class="btn btn-info pull-right">Güncelle</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>






